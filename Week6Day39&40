function teacher(s,c,n,s){
  this.subject=s,
  this.coordinatorName=c,
  this.NOofHours=n,
  this.NoofStudents=s};

  var t=new teacher("Math","Ahmed",23,35);
document.write(t.subject+"</br>");
document.write(t.coordinatorName+"</br>");
document.write(t.NOofHours+"</br>");
document.write(t.NoofStudents);

