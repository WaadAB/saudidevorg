var teacher={
  subject:"",
  coordinatorName:"",
  NOofHours:"",
  NoofStudents:"",
  set subjectvalue(value){
    this.subject=value;
  },
  get subjectback(){
    return this.subject;
  },
  
  set coortvalue(x){
    this.coordinatorName=x;
  },
  get coorback(){
    return this.coordinatorName;
  },
  
  set hourvalue(y){
    this.NOofHours=y;
  },
  get hourback(){
    return this.NOofHours;
  },
  
   set studentvalue(y){
    this.NoofStudents=y;
  },
  get studentback(){
    return this.NoofStudents;
  }
};
teacher.subjectvalue="Physics";
teacher.coortvalue="ahmed";
teacher.hourvalue=20;
teacher.studentvalue=35;
document.write(teacher.subjectback+"</br>");
document.write(teacher.coorback+"</br>");
document.write(teacher.hourback+"</br>");
document.write(teacher.studentback);

